<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OpenTrackerNext.Editor.Views.Editors.Entities.EntityEditorView">
  <Grid ColumnDefinitions="Auto *" RowDefinitions="Auto Auto Auto">
    <Label Classes="EditorLabel" Grid.Column="0" Grid.Row="0" Content="Minimum:" />
    <NumericUpDown x:Name="MinimumNumericUpDown" Classes="EditorNumericUpDown" Grid.Column="1" Grid.Row="0" />

    <Label Classes="EditorLabel" Grid.Column="0" Grid.Row="1" Content="Starting:" />
    <NumericUpDown x:Name="StartingNumericUpDown" Classes="EditorNumericUpDown" Grid.Column="1" Grid.Row="1" />

    <Label Classes="EditorLabel" Grid.Column="0" Grid.Row="2" Content="Maximum:" />
    <NumericUpDown x:Name="MaximumNumericUpDown" Classes="EditorNumericUpDown" Grid.Column="1" Grid.Row="2" />
  </Grid>
</UserControl>
